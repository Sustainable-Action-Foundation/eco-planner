      "url": "https://jesper.cejie.se"
    },
    {
      "name": "Viggo Ström",
      "email": "viggo.strom@sustainable-action.org",
      "url": "https://viggostrom.github.io/"
    }
  ],
  "type": "module",
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.6.0",
    "@prisma/client": "^6.4.1",
    "@types/node": "^20.3.2",
    "accept-language": "^3.0.20",
    "apexcharts": "^4.3.0",
    "bcrypt": "^5.1.1",
    "cookies-next": "^5.1.0",
    "dotenv": "^16.4.5",
    "glob": "^11.0.1",
    "i18next": "^24.2.2",
    "i18next-fs-backend": "^2.6.0",
    "i18next-http-backend": "^3.0.2",
    "iron-session": "^8.0.1",
    "mathjs": "^13.1.1",
    "next": "^14.2.25",
    "nodemailer": "^6.9.14",
    "pocketbase": "^0.21.5",
    "react": "^18.2.0",
    "react-apexcharts": "^1.4.1",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.4.1",
    "sharp": "^0.32.6",
    "typescript": "^5.6.3",
    "use-debounce": "^10.0.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/nodemailer": "^6.4.15",
    "@types/react": "18.2.14",
    "@types/regexp.escape": "^2.0.0",
    "cypress": "^13.7.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.15",
    "lorem-ipsum": "^2.0.8",
    "playwright": "^1.51.1",
    "prisma": "^6.4.1",
    "regexp.escape": "^2.0.1",
    "tsx": "^4.19.2"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": "^v22.7.0"
    }
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
